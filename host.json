{
  "swagger": {
    "enabled": false
  },
  "logger": {
    "categoryFilter": {
      "defaultLevel": "Trace",
      "categoryLevels": {
        "Host.Results": "Error",
        "Function": "Trace",
        "Host.Aggregator": "Information"
      }
    },
    "aggregator": {
      "batchSize": 1000,
      "flushTimeout": "00:00:30"
    }
  },
  "applicationInsights": {
    "sampling": {
      "isEnabled": true,
      "maxTelemetryItemsPerSecond": 5
    }
  },
  "functionTimeout": "00:10:00"
}
